function matrixElementsSum(matrix) {
    var array = [1,1,1,1,1]
    var count = 0
    for (i=0; i<matrix.length ; i++){
       for (j=0; j<matrix[i].length;j++){
          if (matrix[i][j]==0){
            array[j]=0
          } else {
            count +=matrix[i][j]*array[j]
          }
        }
    }
    return count
}
